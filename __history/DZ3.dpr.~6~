program DZ3;

{$APPTYPE CONSOLE}

{$R *.res}

uses
  System.SysUtils;
type car = object
  private mark: string;
  private poe: integer;
  public procedure Init(m: string; p: integer);
  public procedure print;
  public function tax: integer;
end;
function car.tax: integer;
  begin
    result := poe * 12;
  end;
procedure car.Init;
  begin
    mark := m;
    poe := p;
  end;
procedure car.print;
  begin
    writeln('Марка: ', mark);
    writeln('Мощность: ', poe)
  end;
var audi: car;

begin
  audi.Init('Audi', 210);
  writeln('Транспортный налог: ', audi.tax);
  audi.print;
  readln;
end.
