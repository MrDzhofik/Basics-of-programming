unit Distance;

interface
type
  func = function(x: real): real;
  procedure Maxim(f:real; g:real; a:real; b:real; s:real; maxi:real);

implementation
Procedure Maxim;
  var x: real;
  begin
    x := a;
    while x <= b do
      begin
        if abs(f - g) > maxi then
          maxi := abs(f - g);
        x := x + s;

      end;

  end;

end.
