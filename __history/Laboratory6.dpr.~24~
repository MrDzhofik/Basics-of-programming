program Laboratory6;

{$APPTYPE CONSOLE}

{$R *.res}

uses
  System.SysUtils;

var
  str, res: shortstring;
  i, j: integer;
  flag: boolean;
begin
  writeln('¬ведите слова');
  readln(str);
  i := 1;
  str := UpCase(str);
  writeln(str);
  while i <= length(str) do
  begin
    flag := False;
    for j := i to i + 2 do
    begin
      if (str[j] = 'a') or (str[j] = 'E') or (str[j] = 'B') then
        flag := True;
    end;
    if flag then
      begin
      for j := i to i + 6 do res := res + str[j];
      end;

    i := i + 7;
  end;

  writeln(res);


  readln;
end.
