program Laboratory4;

{$APPTYPE CONSOLE}

{$R *.res}

uses
  System.SysUtils;

var
  n, i, j, index, curr: integer;
  p, q: real;
  result, st: string;
  arr: array[1..128] of integer;
begin
  write('Enter n: ');
  readln(n);
  writeln(n);
  index := 1;
  repeat
   writeln('Enter x1 x2 x3: ');
   readln(arr[index], arr[index+1], arr[index+2]);
   index := index + 3;
  until index = 3 * n + 1;

  curr := 1;
  writeln('Enter p, q: ');
  readln(p, q);
  repeat

    if (p >= arr[curr]) and (p <= arr[curr] + arr[curr + 2]) and
    (q <= arr[curr + 1]) and (q >= arr[curr + 1] - arr[curr + 2]) then
      str(curr div 3 + 1, st);
      result := result + ' ' + st;
    curr := curr + 3;
  until curr = 3 * n + 1;
  writeln(result);
  readln;
end.
